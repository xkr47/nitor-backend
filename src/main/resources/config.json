{
  "useNativeOpenSsl": false,
  "tls": {
    "http2": true,
    "serverKey": "certs/localhost.key.clear",
    "serverCert": "certs/localhost.crt",
    "clientChain": "certs/client.chain"
  },
  "basicAuth": {
    "path": "/*",
    "real": "nitor",
    "users": {
      "test": "test",
      "admin": "nimda"
    }
  },
  "proxy": {
    "route": "/proxy/*",
    "host": "example.org",
    "port": 80,
    "path": "/",
    "hostHeader": "example.org"
  }
}